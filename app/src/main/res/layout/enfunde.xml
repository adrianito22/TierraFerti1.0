                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/limpiezaracimo"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/ediDaipaDisco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/dipadisco"
                android:textSize="14sp" />


            <EditText
                android:id="@+id/ediDeschive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/deschive"
                android:textSize="14sp" />


            <EditText
                android:id="@+id/ediCirugiaDeDedos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/cirujiaDedos"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/ediEnfundeBarrera"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/enfundebarrera"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/ediRacimosCorrectos"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/racimoscorrectos"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/ediDestorex"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="@drawable/backrgound_item2"
                android:digits="0123456789."
                android:ems="10"
                android:gravity="center"
                android:inputType="number"
                android:padding="10dp"
                android:tag="enfunde/destore"
                android:textSize="14sp" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>